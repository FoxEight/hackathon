{
  "name": "John and Alex Make Cool Stuff",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/assets/nerd-face.jpeg",
      "32": "/assets/nerd-face.jpeg",
      "48": "/assets/nerd-face.jpeg"
    },
    "icons": {
      "16": "/assets/nerd-face.jpeg",
      "32": "/assets/nerd-face.jpeg",
      "48": "/assets/nerd-face.jpeg"
    }
  }
}
